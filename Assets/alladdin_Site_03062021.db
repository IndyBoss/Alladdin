CREATE ALGORITHM=UNDEFINED DEFINER=`alladdin`@`%` SQL SECURITY DEFINER VIEW `alladdin`.`wp_usergroups` AS (select `a`.`object_id` AS `user_id`,`a`.`group_id` AS `group_id`,`u`.`user_login` AS `user_login`,`g`.`groupname` AS `group_name` from ((`alladdin`.`wp_uam_accessgroup_to_object` `a` left join `alladdin`.`wp_users` `u` on((`a`.`object_id` = `u`.`ID`))) left join `alladdin`.`wp_uam_accessgroups` `g` on((`g`.`ID` = `a`.`group_id`))));
